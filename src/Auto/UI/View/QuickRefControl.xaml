<UserControl x:Class="Losenkov.RegexEditor.UI.View.QuickRefControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:re="clr-namespace:Losenkov.RegexEditor.UI"
             xmlns:v="clr-namespace:Losenkov.RegexEditor.UI.View"
             mc:Ignorable="d"
             d:DesignHeight="800" d:DesignWidth="450">
    <FlowDocumentScrollViewer IsInactiveSelectionHighlightEnabled="True"
                              IsManipulationEnabled="True"
                              >
        <FlowDocument FontFamily="{DynamicResource {x:Static v:QuickRefColors.FontFamilyKey}}"
                      FontSize="{DynamicResource {x:Static v:QuickRefColors.FontSizeKey}}"
                      Background="{DynamicResource {x:Static v:QuickRefColors.DocumentBackgroundBrushKey}}"
                      Foreground="{DynamicResource {x:Static v:QuickRefColors.DocumentForegroundBrushKey}}"
                      PagePadding="8,12"
                      >
            <FlowDocument.Resources>
                <re:EmWidthEvaluator x:Key="width-evaluator" 
                                     FontFamily="{DynamicResource {x:Static v:QuickRefColors.FontFamilyKey}}"
                                     FontSize="{DynamicResource {x:Static v:QuickRefColors.FontSizeKey}}"
                                     FontStretch="Normal"
                                     FontStyle="Italic"
                                     FontWeight="Bold" />
                <re:ColumnWidthConverter x:Key="column-width-converter" />
                <Style x:Key="{x:Type Table}" TargetType="{x:Type Table}">
                    <Setter Property="CellSpacing" Value="0" />
                </Style>
                <Style x:Key="{x:Type TableCell}" TargetType="{x:Type TableCell}">
                    <Setter Property="Padding" Value="1,1" />
                </Style>
                <Style x:Key="{x:Type Paragraph}" TargetType="{x:Type Paragraph}">
                    <Setter Property="TextAlignment" Value="Left" />
                </Style>
                <Style x:Key="row-odd" TargetType="{x:Type TableRow}">
                    <Setter Property="Background" Value="{DynamicResource {x:Static v:QuickRefColors.OddRowBackgroundBrushKey}}" /> <!-- #e0f0e0 -->
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static v:QuickRefColors.OddRowForegroundBrushKey}}" />
                </Style>
                <Style x:Key="row-even" TargetType="{x:Type TableRow}">
                    <Setter Property="Background" Value="{DynamicResource {x:Static v:QuickRefColors.EvenRowBackgroundBrushKey}}" />
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static v:QuickRefColors.EvenRowForegroundBrushKey}}" />
                </Style>
                <Style x:Key="row-head" TargetType="{x:Type TableRow}">
                    <Setter Property="Background" Value="{DynamicResource {x:Static v:QuickRefColors.HeadRowBackgroundBrushKey}}" /> <!-- #80A080 -->
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static v:QuickRefColors.HeadRowForegroundBrushKey}}" /> <!-- White -->
                    <Setter Property="FontWeight" Value="Bold" />
                </Style>
                <Style x:Key="code" TargetType="{x:Type TextElement}">
                    <Setter Property="FontWeight" Value="Bold" />
                </Style>
                <Style x:Key="arg-odd" TargetType="{x:Type Run}">
                    <Setter Property="Background" Value="{DynamicResource {x:Static v:QuickRefColors.OddRowArgBackgroundBrushKey}}" />
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static v:QuickRefColors.OddRowArgForegroundBrushKey}}" />
                    <Setter Property="FontWeight" Value="Normal" />
                    <Setter Property="FontStyle" Value="Italic" />
                </Style>
                <Style x:Key="arg-even" TargetType="{x:Type Run}">
                    <Setter Property="Background" Value="{DynamicResource {x:Static v:QuickRefColors.EvenRowArgBackgroundBrushKey}}" />
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static v:QuickRefColors.EvenRowArgForegroundBrushKey}}" />
                    <Setter Property="FontWeight" Value="Normal" />
                    <Setter Property="FontStyle" Value="Italic" />
                </Style>
                <Style x:Key="column-8" TargetType="{x:Type TableColumn}">
                    <Setter Property="Width" Value="{Binding Source={StaticResource width-evaluator}, Path=EmWidth, Converter={StaticResource column-width-converter}, ConverterParameter=8}" />
                </Style>
                <Style x:Key="column-10" TargetType="{x:Type TableColumn}">
                    <Setter Property="Width" Value="{Binding Source={StaticResource width-evaluator}, Path=EmWidth, Converter={StaticResource column-width-converter}, ConverterParameter=10}" />
                </Style>
                <Style x:Key="column-14" TargetType="{x:Type TableColumn}">
                    <Setter Property="Width" Value="{Binding Source={StaticResource width-evaluator}, Path=EmWidth, Converter={StaticResource column-width-converter}, ConverterParameter=14}" />
                </Style>
            </FlowDocument.Resources>
            <re:CollapsibleSection HeaderText="SINGLE CHARACTERS"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/character-escapes-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{StaticResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To match any character</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    [<Run Style="{StaticResource arg-odd}" Text="set" />]
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>In that set</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    [^<Run Style="{StaticResource arg-even}" Text="set"/>]
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Not in that set</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    [<Run Style="{StaticResource arg-odd}" Text="a-z"/>]
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>In the a-z range</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    [^<Run Style="{StaticResource arg-even}" Text="a-z"/>]
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Not in the a-z range</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">.</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Any except \n (new line)</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \<Run Style="{StaticResource arg-even}" Text="char" />
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Escaped special character</Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
            </re:CollapsibleSection>
            <re:CollapsibleSection HeaderText="CONTROL CHARACTERS"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/character-escapes-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{DynamicResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Or</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To match</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\t</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u0009</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Horizontal tab</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\v</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u000B</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Vertical tab</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\b</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u0008</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Backspace</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\e</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u001B</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Escape</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\r</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u000D</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Carriage return</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\f</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u000C</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Form feed</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\n</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u000A</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>New line</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\a</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\u0007</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Bell (alarm)</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \c<Run Style="{StaticResource arg-odd}" Text="char" />
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                            </TableCell>
                            <TableCell>
                                <Paragraph>ASCII control character</Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
            </re:CollapsibleSection>
            <re:CollapsibleSection HeaderText="NON-ASCII CODES"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/character-escapes-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{DynamicResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To match character with</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \<Run Style="{StaticResource arg-odd}" Text="octal" />
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>2-3 digit octal character code</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \x<Run Style="{StaticResource arg-even}" Text="hex" />
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>2-digit hex character code</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \u<Run Style="{StaticResource arg-odd}" Text="hex" />
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>4-digit hex character code</Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
            </re:CollapsibleSection>
            <re:CollapsibleSection HeaderText="CHARACTER CLASSES"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/character-classes-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{DynamicResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To match any character</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \p{<Run Style="{StaticResource arg-odd}" Text="cat" />}
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>In that Unicode category or block</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    \P{<Run Style="{StaticResource arg-even}" Text="cat" />}
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Not in that Unicode category or block</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\w</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Word character</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\W</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Non-word character</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\d</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Decimal digit</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\D</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Not a decimal digit</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\s</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>White-space character</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\S</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Non-white-space char</Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
            </re:CollapsibleSection>
            <re:CollapsibleSection HeaderText="GREEDY QUANTIFIERS"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/quantifiers-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{DynamicResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To match</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">*</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>0 or more times</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">+</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>1 or more times</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">?</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>0 or 1 time</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    {<Run Style="{StaticResource arg-even}" Text="n" />}
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>
                                    Exactly <Run Style="{StaticResource arg-even}" Text="n" /> times
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    {<Run Style="{StaticResource arg-odd}" Text="n" />,}
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>
                                    At least <Run Style="{StaticResource arg-odd}" Text="n" /> times
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    {<Run Style="{StaticResource arg-even}" Text="n" />,<Run Style="{StaticResource arg-even}" Text="m" />}
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>
                                    From
                                    <Run Style="{StaticResource arg-even}" Text="n" /> to
                                    <Run Style="{StaticResource arg-even}" Text="m" /> times
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
            </re:CollapsibleSection>
            <re:CollapsibleSection HeaderText="LAZY QUANTIFIERS"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/quantifiers-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{DynamicResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To match</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">*?</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>0 or more times</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">+?</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>1 or more times</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">??</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>0 or 1 time</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    {<Run Style="{StaticResource arg-even}" Text="n" />}?
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>
                                    Exactly <Run Style="{StaticResource arg-even}" Text="n" /> times
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    {<Run Style="{StaticResource arg-odd}" Text="n" />,}?
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>
                                    At least <Run Style="{StaticResource arg-odd}" Text="n" /> times
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">
                                    {<Run Style="{StaticResource arg-even}" Text="n" />,<Run Style="{StaticResource arg-even}" Text="m" />}?
                                </Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>
                                    From
                                    <Run Style="{StaticResource arg-even}" Text="n" /> to
                                    <Run Style="{StaticResource arg-even}" Text="m" /> times
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
            </re:CollapsibleSection>
            <re:CollapsibleSection HeaderText="ANCHORS"
                                   HeaderUri="https://docs.microsoft.com/en-us/dotnet/standard/base-types/anchors-in-regular-expressions"
                                   >
                <Table>
                    <Table.Columns>
                        <TableColumn Style="{StaticResource column-8}" />
                        <TableColumn />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow Style="{DynamicResource row-head}">
                            <TableCell>
                                <Paragraph>Use</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>To specify position</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">^</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>At start of string or line</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\A</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>At start of string</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\z</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>At end of string</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\Z</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>At end (or before \n at end) of string</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">$</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>At end (or before \n at end) of string or line</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\G</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Where previous match ended</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-odd}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\b</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>On word boundary</Paragraph>
                            </TableCell>
                        </TableRow>
                        <TableRow Style="{StaticResource row-even}">
                            <TableCell>
                                <Paragraph Style="{StaticResource code}">\B</Paragraph>
                            </TableCell>
                            <TableCell>
                                <Paragraph>Not on word boundary</Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
